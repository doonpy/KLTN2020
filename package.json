{
  "name": "gt2020-serverside",
  "version": "1.0.0",
  "description": "Graduation thesis 2020 - server side",
  "scripts": {
    "clean": "rimraf dist",
    "dev:api": "nodemon --watch api-server/src --ext ts --exec ts-node --project api-server/tsconfig.json api-server/src/server.ts",
    "dev:bgr-job": "nodemon --watch api-server/src --ext ts --exec ts-node --project api-server/tsconfig.json api-server/src/background-job/main.ts",
    "dev:web": "next web-server -p 3001",
    "start:api": "node dist/api-bgr-job/server.js",
    "start:bgr-job": "node dist/api-bgr-job/background-job/main.js",
    "start:web": "next start dist/web -p 3001",
    "build": "npm run clean && npm-run-all -p build:api build:web --print-label",
    "build:api": "tsc --build --extendedDiagnostics api-server",
    "build:web": "next build web-server",
    "eslint": "eslint -c .eslintrc 'web-server/**/*.ts' 'api-server/**/*.ts'",
    "eslint:fix": "eslint --fix -c .eslintrc 'web-server/**/*.ts' 'api-server/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doonpy/KLTN2020.git"
  },
  "author": "PK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doonpy/KLTN2020/issues"
  },
  "homepage": "https://github.com/doonpy/KLTN2020#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "highcharts": "^8.0.4",
    "highcharts-react-official": "^3.0.0",
    "jquery": "^3.5.1",
    "mongoose": "^5.9.11",
    "mongoose-auto-increment": "^5.0.1",
    "next": "^9.3.6",
    "node-telegram-bot-api": "^0.40.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "swr": "^0.2.0",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jquery": "^3.3.38",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.7.14",
    "@types/mongoose-auto-increment": "^5.0.33",
    "@types/node": "^13.13.4",
    "@types/node-telegram-bot-api": "^0.40.3",
    "@types/react": "^16.9.34",
    "@types/request-promise": "^4.1.46",
    "@types/socket.io": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
