{
  "name": "kltn2020",
  "version": "1.0.0",
  "description": "Graduation thesis 2020",
  "scripts": {
    "clean": "rimraf dist",
    "tsc:api": "tsc -p api-server",
    "run-dev:api": "npm run tsc:api && npm run start:api",
    "run-dev:bgr-job": "npm run tsc:api && npm run start:bgr-job",
    "run-dev:init-data": "npm run tsc:api && npm run start:init-data",
    "dev:api": "nodemon -w api-server/src -e ts --exec 'npm run run-dev:api'",
    "dev:bgr-job": "nodemon -w api-server/src -e ts --exec 'npm run run-dev:bgr-job'",
    "dev:init-data": "nodemon -w api-server/src -e ts --exec 'npm run run-dev:init-data'",
    "dev:web": "next dev web-server -p 3001",
    "start:init-data": "node dist/api-bgr-job/data-initialization.js",
    "start:api": "node dist/api-bgr-job/server.js",
    "start:bgr-job": "node dist/api-bgr-job/background-job/main.js",
    "start:web": "next start dist/web -p 3001",
    "build": "npm run clean && npm run build:api && npm run build:web",
    "build:api": "tsc --build --extendedDiagnostics api-server",
    "build:web": "next build web-server",
    "eslint": "eslint --max-warnings=0 . --ext .ts,.js",
    "eslint:fix": "eslint --fix . --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doonpy/KLTN2020.git"
  },
  "author": "PK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doonpy/KLTN2020/issues"
  },
  "homepage": "https://github.com/doonpy/KLTN2020#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "highcharts": "^8.1.0",
    "highcharts-react-official": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "leaflet": "^1.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.sortby": "^4.7.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.17",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-autopopulate": "^0.12.2",
    "next": "^9.4.4",
    "next-fonts": "^1.1.0",
