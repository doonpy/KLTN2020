{
  "name": "gt2020-serverside",
  "version": "1.0.0",
  "description": "Graduation thesis 2020 - server side",
  "scripts": {
    "clean": "rimraf dist",
    "dev:api": "cd api-server && ts-node src/server.ts",
    "dev:bgr-job": "cd api-server && ts-node src/bgr-job/background-job.ts",
    "dev:web": "next web-server -p 3001",
    "start:api": "node dist/api-bgr-job/server.js",
    "start:bgr-job": "node --max-old-space-size=8192 dist/api-bgr-job/bgr-job/background-job.js",
    "start:web": "next start dist/web -p 3001",
    "build": "npm-run-all clean tslint build:api-bgr-job build:web --print-label",
    "build:api-bgr-job": "tsc --build --extendedDiagnostics api-server",
    "build:web": "next build web-server",
    "tslint": "tslint --fix -c tslint.json 'web-server/**/*.ts' 'api-server/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doonpy/KLTN2020.git"
  },
  "author": "PK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doonpy/KLTN2020/issues"
  },
  "homepage": "https://github.com/doonpy/KLTN2020#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.9",
    "mongoose-auto-increment": "^5.0.1",
    "next": "^9.3.5",
    "node-telegram-bot-api": "^0.40.0",
    "path": "^0.12.7",
    "pug": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "ts-node": "^8.8.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.17",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.10",
    "@types/mongoose-auto-increment": "^5.0.32",
    "@types/node": "^13.11.1",
    "@types/node-telegram-bot-api": "^0.40.3",
    "@types/react": "^16.9.34",
    "@types/request-promise": "^4.1.46",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.8.3"
  }
}
