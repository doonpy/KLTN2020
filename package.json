{
  "name": "gt2020-serverside",
  "version": "1.0.0",
  "description": "Graduation thesis 2020 - server side",
  "scripts": {
    "clean": "rimraf dist",
    "dev:initialize-data": "cd api-server/src && ts-node data-initialization.ts",
    "tsc:api": "tsc -w -p api-server",
    "watcher:api": "nodemon -w dist/api-bgr-job --exec 'ts-node -P api-server/tsconfig.json api-server/src/server.ts'",
    "watcher:bgr-job": "nodemon -w api-server/src -e ts --exec 'ts-node -P api-server/tsconfig.json api-server/src/background-job/main.ts'",
    "dev:api": "npm-run-all -p tsc:api watcher:api --print-label",
    "dev:bgr-job": "npm-run-all -p tsc:api watcher:bgr-job --print-label",
    "dev:web": "next web-server -p 3001",
    "start:initialize-data": "NODE_ENV=production node dist/api-bgr-job/data-initialization.js",
    "start:api": "node dist/api-bgr-job/server.js",
    "start:bgr-job": "node dist/api-bgr-job/background-job/main.js",
    "start:web": "next start dist/web -p 3001",
    "build": "npm run clean && npm-run-all -p build:api build:web --print-label",
    "build:api": "tsc --build --extendedDiagnostics api-server",
    "build:web": "next build web-server",
    "eslint": "eslint -c .eslintrc web-server/**/*.js api-server/**/*.ts",
    "eslint:fix": "eslint --fix -c .eslintrc web-server/**/*.js api-server/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doonpy/KLTN2020.git"
  },
  "author": "PK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doonpy/KLTN2020/issues"
  },
  "homepage": "https://github.com/doonpy/KLTN2020#readme",
  "dependencies": {
    "@highcharts/map-collection": "^1.1.3",
    "@types/puppeteer": "^2.1.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "highcharts": "^8.1.0",
    "highcharts-react-official": "^3.0.0",
    "jquery": "^3.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.13",
    "mongoose-auto-increment": "^5.0.1",
    "next": "^9.4.0",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
    "next-redux-wrapper": "^6.0.0-rc.7",
    "node-telegram-bot-api": "^0.40.0",
    "path": "^0.12.7",
    "proj4": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "swr": "^0.2.0",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/cheerio": "^0.22.18",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.7.16",
    "@types/mongoose-auto-increment": "^5.0.33",
    "@types/node": "^13.13.5",
    "@types/node-telegram-bot-api": "^0.40.3",
    "@types/react": "^16.9.35",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@root": "dist/api-bgr-job",
    "@background-job": "dist/api-bgr-job/background-job",
    "@common": "dist/api-bgr-job/common",
    "@middleware": "dist/api-bgr-job/middleware",
    "@service": "dist/api-bgr-job/service",
    "@util": "dist/api-bgr-job/util"
  }
}
